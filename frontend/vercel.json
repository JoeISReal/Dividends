{
    "rewrites": [
        {
            "source": "/api/holders",
            "destination": "/api/holders.js"
        },
        {
            "source": "/api/test",
            "destination": "/api/test.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}